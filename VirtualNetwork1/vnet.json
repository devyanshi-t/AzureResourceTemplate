{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "Vnetname": {
            "type": "string"},
        "location": {
            "type": "string"
        },
        "VnetaddressPrefix": {
            "type": "string"
        },
        "subnetName1": {
            "type": "string"
        },
        "subnetName2": {
            "type": "string"
        },
    
        "subnetAddressPrefix1": {
            "type": "string"
        },
        "subnetAddressPrefix2": {
            "type": "string"
        },
        "subnetAddressPrefix3": {
            "type": "string"
        },
        
        "enableDdosProtection": {
            "type": "bool"
        }
    },
    "resources": [
        {
            "apiVersion": "2019-02-01",
            "name": "[parameters('Vnetname')]",
            "type": "Microsoft.Network/virtualNetworks",
            "location": "[parameters('location')]",
            "properties": {
                "VnetaddressSpace": {
                    "addressPrefixes": [
                        "[parameters('VnetaddressPrefix')]"
                    ]
                },
                "subnets": [
                    {
                        "name": "[parameters('subnetName1')]",
                        "properties": {
                            "addressPrefix": "[parameters('subnetAddressPrefix1')]"
                        }
                    },
                    
                    {
                        "name":"[parameters('subnetName2')]",
                        "properties":{
                            
                            "addressPrefix": "[parameters('subnetAddressPrefix2')]"
                        }
                        
                        
                        
                        
                    },
                    {
                        "name":"AzureFirewallSubnet",
                        "properties":{
                            
                            "addressPrefix": "[parameters('subnetAddressPrefix3')]"
                        }
                    }
                ],
                "enableDdosProtection": "[parameters('enableDdosProtection')]"
            }
        }
    ],
    "outputs": {}
}
