{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "Vnetname": {
            "type": "string"},
        "location": {
            "type": "string",
            "defaultValue": "[resourceGroup().location]",
            "metadata": {
                "description": "Location for all resources."
            }
        },
        "VnetaddressPrefix": {
            "type": "string",
            "defaultValue": "10.0.0.0/16",
            "metadata": {
                "description": " Virtual Network Address prefix"
            }
            },
        "subnetName1": {
            "type": "string"
        },
        "subnetName2": {
            "type": "string"
        },
    
        "firewallSubnetAddressPrefix": {
            "type": "string",
            "defaultValue": "10.0.1.0/24",
            "metadata": {
                "description": "Address  Prefix of the firewall subnet"
            }
           
        },
        "subnetAddressPrefix1": {
            "type": "string",
           "defaultValue": "10.0.2.0/24",
            "metadata": {
                "description": "Subnet-1 Prefix"
            }
        },
        "subnetAddressPrefix2": {
        
            "type": "string",
            "defaultValue": "10.0.3.0/24",
            "metadata": {
                "description": "Subnet-2 Prefix"
            }
        },
        
        "enableDdosProtection": {
            "type": "bool"
        }
    },
    "resources": [
        {
            "apiVersion": "2019-02-01",
            "name": "[parameters('Vnetname')]",
            "type": "Microsoft.Network/virtualNetworks",
            "location": "[parameters('location')]",
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "[parameters('VnetaddressPrefix')]"
                    ]
                },
                "subnets": [
                    {
                        "name": "[parameters('subnetName1')]",
                        "properties": {
                            "addressPrefix": "[parameters('subnetAddressPrefix1')]"
                        }
                    },
                    
                    {
                        "name":"[parameters('subnetName2')]",
                        "properties":{
                            
                            "addressPrefix": "[parameters('subnetAddressPrefix2')]"
                        }
                        
                        
                        
                        
                    },
                    {
                        "name":"AzureFirewallSubnet",
                        "properties":{
                            
                            "addressPrefix": "[parameters('firewallSubnetAddressPrefix')]"
                        }
                    }
                ],
                "enableDdosProtection": "[parameters('enableDdosProtection')]"
            }
        }
    ],
    "outputs": {}
}
